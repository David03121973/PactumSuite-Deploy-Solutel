{
  "name": "proyecto_example",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-server": "^14.1.1",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.20.1",
    "path": "^0.12.7",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "pkg": "^5.8.1",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "scripts": {
    "dev": "nodemon app.js",
    "start": "node app.js",
    "build": "pkg app.js --targets node16-win-x64 --out-path dist && node scripts/build.js"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "config/**/*",
      "views/**/*",
      "public/**/*"
    ],
    "scripts": [
      "node_modules/pg/**/*.js",
      "node_modules/pg-hstore/**/*.js"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.3"
  }
}
